{
  "name": "web-console",
  "version": "1.0.0",
  "description": "A web console to manage your computer remotely, tailored for Raspberry Pi devices",
  "author": "Fernando Andreu",
  "private": true,
  "scripts": {
    "test": "jest",
    "dev-watch": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "dev": "cross-env NODE_ENV=development node server/index.js",
    "dev-external": "cross-env NODE_ENV=development NUXT_HOST=0.0.0.0 NUXT_PORT=3000 node server/index.js",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production NUXT_HOST=0.0.0.0 NUXT_PORT=3000 node server/index.js",
    "generate": "nuxt generate"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "express-ws": "^4.0.0",
    "node-pty": "^0.8.1",
    "nuxt": "^2.0.0",
    "nuxt-basic-auth-module": "^1.2.0",
    "ts-node": "latest",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "xterm": "beta",
    "xterm-addon-attach": "beta",
    "xterm-addon-fit": "beta",
    "xterm-addon-search": "beta",
    "xterm-addon-web-links": "beta",
    "xterm-addon-webgl": "beta"
  },
  "devDependencies": {
    "@nuxt/typescript": "latest",
    "@types/jest": "^24.0.15",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "jest": "^24.1.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.3",
    "vue-jest": "^3.0.3"
  }
}
